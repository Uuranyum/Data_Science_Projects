    "def remove_punc(text):\n",
    "    punc = string.punctuation\n",
    "    clean_text = text.translate(str.maketrans('','', punc))\n",
    "    return clean_text\n",
    "\n",
    "df['content'] = df['content'].apply(remove_punc)\n",
    "df['content'].head(20)"
   ]
